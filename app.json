{
  "expo": {
    "name": "Smart tootsoo",
    "slug": "Naimaa-naimaa",
    "version": "2.0.0",
    "orientation": "portrait",
    "icon": "./src/assets/ioslogo.png",
    "scheme": "myapp",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./src/assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "net.naimaa",
      "buildNumber": "2.0.0",
      "entitlements": {
        "com.apple.developer.networking.wifi-info": true
      },
      "infoPlist": {
        "NSPhotoLibraryUsageDescription": "Access to your photo library is required to attach photos to scan qr code or product images upload.",
        "NSCameraUsageDescription": "Naimaa requires access to your camera to goods photo to upload and goods scanning qr code"
      }
    },
    "android": {
      "versionCode": 40,
      "package": "mn.naimaa",
      "permissions": [
        "CAMERA",
        "WRITE_EXTERNAL_STORAGE",
        "CAMERA_ROLL"
      ],
      "adaptiveIcon": {
        "foregroundImage": "./src/assets/androidlogo.png",
        "backgroundColor": "#ffffff"
      }
    },
    "extra": {
      "eas": {
        "projectId": "fb881caa-1bd7-4960-abe0-3613ab65ff98"
      }
    }
  }
}
